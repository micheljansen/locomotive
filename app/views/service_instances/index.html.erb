<h1>Listing service_instances</h1>

<table>
  <tr>
  </tr>

<% for service_instance in @service_instances %>
  <tr>
    <td><%= link_to 'Show', service_instance %></td>
    <td><%= link_to 'Edit', edit_service_instance_path(service_instance) %></td>
    <td><%= link_to 'Destroy', service_instance, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New service_instance', new_service_instance_path %>
