<h1>Listing release_versions</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% for release_version in @release_versions %>
  <tr>
    <td><%=h release_version.name %></td>
    <td><%= link_to 'Show', release_version %></td>
    <td><%= link_to 'Edit', edit_release_version_path(release_version) %></td>
    <td><%= link_to 'Destroy', release_version, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New release_version', new_release_version_path %>
