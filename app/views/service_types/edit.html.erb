<h1>Editing service_type</h1>

<% form_for(@service_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @service_type %> |
<%= link_to 'Back', service_types_path %>
