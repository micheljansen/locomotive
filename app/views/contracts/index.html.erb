<h1>Listing contracts</h1>

<table>
  <tr>
  </tr>

<% for contract in @contracts %>
  <tr>
    <td><%= link_to 'Show', contract_path(contract.id) %></td>
    <td><%= link_to 'Edit', edit_contract_path(contract.id) %></td>
    <td><%= link_to 'Destroy', contract_path(contract.id), :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New contract', new_contract_path %>
