<h1>Listing contracts</h1>

<table>
  <tr>
  </tr>

<% for contract in @contracts %>
  <tr>
    <td><%= link_to 'Show', resource(contract) %></td>
    <td><%= link_to 'Edit', resource(contract, :edit) %></td>
    <td><%= link_to 'Delete', resource(contract, :delete), :class => 'delete-link' %></td>
  </tr>
<% end %>
</table>

