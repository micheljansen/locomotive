<h1>Listing application_instances</h1>

<table>
  <tr>
  </tr>

<% for application_instance in @application_instances %>
  <tr>
    <td><%= link_to 'Show', application_instance %></td>
    <td><%= link_to 'Edit', edit_application_instance_path(application_instance) %></td>
    <td><%= link_to 'Destroy', application_instance, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

