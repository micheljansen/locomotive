<h1>Editing dependency</h1>

<% form_for(@dependency) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :role_id %><br />
    <%= f.text_field :role_id %>
  </p>
  <p>
    <%= f.label :service_type_id %><br />
    <%= f.text_field :service_type_id %>
  </p>
  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @dependency %> |
<%= link_to 'Back', dependencies_path %>
